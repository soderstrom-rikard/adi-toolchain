<?xml version="1.0" encoding="ISO-8859-1"?>
<template
	type="ProjTempl"
	version="1.0"
	supplier="blackfin.uclinux.org"
	revision="1.0"
	author="Mike Frysinger (Analog Devices Inc.)"
	copyright="Copyright (c) 2008 Analog Devices Inc."
	id="@ID@"
	label="%@ID@.label"
	description="%@ID@.description"
>

	<process type="org.eclipse.cdt.managedbuilder.core.NewManagedProject">
		<simple name="name" value="$(projectName)"/>
		<simple name="artifactExtension" value="exe"/>
		<simple name="isCProject" value="true"/>
	</process>

	<!-- doesnt seem to work :(
	<process type="org.eclipse.cdt.managedbuilder.core.SetMBSStringOptionValue">
		<simple name="projectName" value="$(projectName)"/>
		<complex-array name="resourcePaths">
			<simple name="id" value="com.adi.toolchain.gnu.bfin.options.general.proc"/>
			<simple name="value" value="com.adi.toolchain.gnu.bfin.proc.@CPU@"/>
			<simple name="path" value=""/>
		</complex-array>
	</process>
	-->

	<process type="org.eclipse.cdt.core.CreateSourceFolder">
		<simple name="projectName" value="$(projectName)"/>
		<simple name="path" value="src"/>
	</process>

	<process type="org.eclipse.cdt.core.AddFiles">
		<simple name="projectName" value="$(projectName)"/>
		<complex-array name="files">
@FILES@
		</complex-array>
	</process>

</template>
